datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  birthdate DateTime?
  first_name String?
  last_name  String?
  gender    String?  @default("")
  user_name  String?  @default("")
}

model Disease {
  id   Int    @id @default(autoincrement())
  name String @unique
}
